FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      H+;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -3 0 0 1 0 0];

internalField   uniform 0;

boundaryField
{
	walls
    {
        type            zeroGradient;//globalConcentrationMixed;
    }

	solidwalls
    {
        type            zeroGradient;//globalConcentrationMixed;
    }

	inlet
    {
        type            fixedValue;
        value           uniform cinlet;
    }
	
	outlet
    {
        type            zeroGradient;
    }

    frontAndBack
    {
        type empty;

    }
}

